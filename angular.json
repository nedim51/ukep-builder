{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ukep-builder": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ukep-builder",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["src/assets/styles"]
            },
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              
              "src/assets/styles/flexbox-grid.scss",
              "src/assets/styles/flexbox-grid-xs.scss",
              "src/assets/styles/flexbox-grid-sm.scss",
              "src/assets/styles/flexbox-grid-md.scss",
              "src/assets/styles/flexbox-grid-lg.scss",
              
              "src/assets/styles/icons.scss",
              "src/assets/styles/scrollbar.scss",
              
              "src/assets/styles/selection.scss",
              "src/assets/styles/utils.scss",
              
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ukep-builder:build:production"
            },
            "development": {
              "buildTarget": "ukep-builder:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ukep-builder:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/styles/flexbox-grid.scss",
              "src/assets/styles/flexbox-grid-xs.scss",
              "src/assets/styles/flexbox-grid-sm.scss",
              "src/assets/styles/flexbox-grid-md.scss",
              "src/assets/styles/flexbox-grid-lg.scss",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ng-ukep-icons": {
      "projectType": "library",
      "root": "projects/ng-ukep-icons",
      "sourceRoot": "projects/ng-ukep-icons/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-ukep-icons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-ukep-icons/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-ukep-icons/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-ukep-icons/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
